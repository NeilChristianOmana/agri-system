use farmers_info;

CREATE TABLE fisheries_anao (
    id INT AUTO_INCREMENT PRIMARY KEY,
    farmer_name VARCHAR(100),
    tilapia_ponds INT DEFAULT 0,
    tilapia_area DOUBLE DEFAULT 0,
    tilapia_stocks INT DEFAULT 0,
    bangus_ponds INT DEFAULT 0,
    bangus_area DOUBLE DEFAULT 0,
    bangus_stocks INT DEFAULT 0,
    catfish_ponds INT DEFAULT 0,
    catfish_area DOUBLE DEFAULT 0,
    catfish_stocks INT DEFAULT 0,
    crawfish_ponds INT DEFAULT 0,
    crawfish_area DOUBLE DEFAULT 0,
    crawfish_stocks INT DEFAULT 0,
    total_pond_area DOUBLE DEFAULT 0,
    total_avg_production DOUBLE DEFAULT 0,
    tilapia_feeds INT DEFAULT 0,
    bangus_feeds INT DEFAULT 0,
    catfish_feeds INT DEFAULT 0,
    crawfish_feeds INT DEFAULT 0,
    total_feeds_used INT DEFAULT 0,
    date_recorded DATE DEFAULT (CURRENT_DATE)
);

